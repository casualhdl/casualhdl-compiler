library chdl.arithmetic
entity adder32

port clk = clock()
port reset = reset()
port a = vector(31, 0)
port b = vector(31, 0)
port sum = vector(31, 0)

proc adder = sync(clk, reset):
    reset:
        sum = 0

    sum = a + b